{
  "name": "vix-oracle",
  "version": "0.0.1",
  "description": "vix oracle",
  "scripts": {
    "lint": "prettier --check ./src/{**/*,*}.js ./test/{**/*,*}.js ./contracts/{**/*,**/**/*}.sol ",
    "lint:fix": "prettier  --write ./src/{**/*,*}.js ./test/{**/*,*}.js ./contracts/{**/*,**/**/*}.sol",
    "vic:deploy": "hardhat --network viction deploy",
    "vic:export": "hardhat --network viction export --export exports/viction.json",
    "vic:verify": "hardhat --network viction etherscan-verify --solc-input",
    "compile": "hardhat compile",
    "coverage": "hardhat coverage",
    "test": "hardhat test"
  },
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.0",
    "@nomiclabs/hardhat-solhint": "^3.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.8.2",
    "@types/chai": "^4.2.6",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethereumjs-util": "^7.0.10",
    "ethers": "^5.1.0",
    "hardhat": "^2.6.7",
    "hardhat-deploy": "^0.9.4",
    "mocha": "^6.2.2",
    "prettier": "^2.0.4",
    "prettier-plugin-solidity": "^1.0.0-alpha.57",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@pythnetwork/pyth-evm-js": "^1.29.2",
    "@pythnetwork/pyth-sdk-solidity": "^2.3.0"
  }
}